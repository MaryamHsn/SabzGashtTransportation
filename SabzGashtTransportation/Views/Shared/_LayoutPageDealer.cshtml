                <a href="#"><img src="images/in_icon.png" alt=""></a>
            </div>
        </div>
        <div class="bg_navigation">
            <div class="navigation_wrapper">
                <div id="navigation">
                    <span>صفحه نخست</span>
                    <ul>
                        <li class="current"> @Html.ActionLink("صفحه نخست", "index", "Home", null, null) </li>
                        <li> @Html.ActionLink("درباره ما", "index", "Rout", null, null) </li>
                        <li>  @Html.ActionLink("ورود", "login", "Account", null, null) </li>
                        <li>  @Html.ActionLink("ثبت نام", "Register", "Account", null, null) </li>
                    </ul>
                </div>
                <div id="search_form">
                    <form method="get" action="#">
                        <input type="text" onblur="if(this.value=='') this.value='Search on site';" onfocus="if(this.value=='Search on site') this.value='';" value="Search on site" class="txb_search" />
                        <input type="submit" value="Search" class="btn_search" />
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!--EOF HEADER-->
    <!--BEGIN CONTENT-->
    <!--ترجمه شده توسط مرجع تخصصی برنامه نویسان-->
